{
  "name": "my-app",
  "version": "0.1.0",
  "description": "",
  "author": {
    "name": "tttoh",
    "email": "tttoh88@gmail.com"
  },
  "homepage": "https://tttoh.github.io/my-app",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "predeploy": "rmdir /q /s dist && parcel build index.html --public-url https://tttoh.github.io/my-app",
    "deploy": "gh-pages -d dist"
  },
  "license": "MIT",
  "dependencies": {
    "gh-pages": "^2.0.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-static-files-copy": "^2.0.0"
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  },
  "staticFiles": {
    "staticPath": "assets",
    "watcherGlob": "**"
  }
}
